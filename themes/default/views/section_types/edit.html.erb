<h1>Editing section_type</h1>

<% form_for(@section_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @section_type %> |
<%= link_to 'Back', section_types_path %>